<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>app-gallery</name>
  <displayName>Gallery App</displayName>
  <description>Gallery Content App</description>
  <version>1.0.0-SNAPSHOT</version>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-admincentral</name>
      <version>5.4/*</version>
    </dependency>
    <dependency>
      <name>ui-contentapp</name>
      <version>5.4/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>dam-app</name>
      <version>2.1/*</version>
    </dependency>
  </dependencies>


  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>galleryelements</workspace>
      </workspaces>
      <nodeTypeFile>/mgnl-nodetypes/app-gallery-nodetypes.xml</nodeTypeFile>
    </repository>
  </repositories>

</module>